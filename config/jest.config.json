{
  "collectCoverageFrom": ["src/**/*.ts"],
  "testMatch": ["<rootDir>/test/**/*.ts"],
  "testEnvironment": "node",
  "testURL": "http://localhost",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "moduleFileExtensions": ["ts", "json", "js"],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  }
}
