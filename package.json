{
  "name": "ornn",
  "version": "0.0.0",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "description": "SQL ORM for node",
  "keywords": ["node", "orm", "sql", "postgres"],
  "repository": "xiaofan2406/ornn",
  "bugs": {
    "url": "https://github.com/xiaofan2406/ornn/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint:eslint": "eslint config src test",
    "lint:prettier": "prettier-check '{config,src,test}/**/*.{js,json,md}'",
    "lint": "npm run lint:prettier",
    "format": "prettier '{config,src,test}/**/*.{js,json,md}' --write",
    "cleanup": "rm -rf lib",
    "docker": "docker-compose up",
    "docker-down": "docker-compose down",
    "prebuild": "npm run cleanup",
    "build": "NODE_ENV=production babel src --out-dir lib",
    "test": "NODE_ENV=test jest --watch",
    "coverage": "NODE_ENV=test jest --coverage",
    "prepublishOnly": "npm run build",
    "postpublish": "git push --follow-tags",
    "repl": "babel-node"
  },
  "jest": {
    "preset": "<rootDir>/config/jest.config.json"
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier --write", "git add"]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "pg": "^7.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.13.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-jest": "^1.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.1",
    "prettier-check": "^2.0.0"
  }
}
